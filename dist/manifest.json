{
    "manifest_version": 3,
    "name": "BookmarksManager",
    "version": "1.0.0",
    "icons": {
        "16": "img/icon_16.png",
        "48": "img/icon_48.png"
    },
    "permissions": [
        "tabs",
        "bookmarks"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "img/icon_16.png",
            "32": "img/icon_32.png",
            "48": "img/icon_48.png"
        },
        "default_title": "Bookmarks Manager",
        "default_popup": "index.html"
    }

}